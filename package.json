{
  "name": "protractor-cucumber-template",
  "version": "1.0.0",
  "description": "This project sets and uses the protractor-cucumber-framework to BBD with cucumber and protractor",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json '**/*.ts'",
    "build": "tsc",
    "pretest": "npm run lint && npm run build",
    "test": "protractor ./dist/protractor.conf.js",
    "postinstall": "webdriver-manager update --gecko=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luigisamurai/protractor-cucumber-template.git"
  },
  "keywords": [
    "BDD",
    "Cucumber",
    "Protractor",
    "template"
  ],
  "author": "labs.sistemas@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luigisamurai/protractor-cucumber-template/issues"
  },
  "homepage": "https://github.com/luigisamurai/protractor-cucumber-template#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/config": "0.0.34",
    "@types/cucumber": "^4.0.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "config": "^3.0.1",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.0",
    "protractor": "^5.4.2",
    "protractor-cucumber-framework": "^6.1.1",
    "protractor-retry": "^1.2.6",
    "ts-node": "^8.0.1",
    "tslint": "^5.12.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-eslint-rules-recommended": "^1.2.0",
    "typescript": "^3.2.4"
  }
}
